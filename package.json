        "@nestjs/websockets": "^8.2.3",
        "@sendgrid/mail": "^7.4.7",
        "axios": "^0.24.0",
        "bcrypt": "^5.0.1",
        "correios-brasil": "^2.2.2",
        "env-cmd": "^10.1.0",
