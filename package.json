        "randomstring": "^1.2.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.6",
        "uninstall": "0.0.0",
        "uuid": "^8.3.2",
        "xhr2": "^0.2.1",
        "xml2js": "^0.4.23"
